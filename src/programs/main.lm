// Import system API and other libraries.
import!("lib/main.lm");

// Import all programs.
import!("shell.lm");

// Register programs.
display folder = "Programs";
export const p_rograms = [stackid(s_hell)];

display folder = "Programs";
action r_un_shell = n_ext_process_stack {
    let newStack = DEFAULT_PROCESS_STACK;
    newStack[1] = p_rograms[1];

    newStack
}